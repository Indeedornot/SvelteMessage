{
	"name": "sveltemessage",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"predev": "npm run prisma:migrate:dev",
		"dev": "vite dev --host",
		"build": "vite build",
		"preview": "vite preview",
		"postinstall": "prisma generate && npm run prisma:postMigrate",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint:fix": "prettier --plugin-search-dir . --check . && eslint .",
		"lint:format": "prettier --config prettier.config.cjs --write --plugin-search-dir=. .",
		"lint": "npm run lint:format && npm run lint:fix",
		"prisma:migrate:dev": "prisma migrate dev && npm run prisma:postMigrate",
		"prisma:migrate:reset": "prisma migrate reset --force && npm run prisma:postMigrate",
		"prisma:postMigrate": "ts-node --esm ./prisma/postMigrate.ts"
	},
	"devDependencies": {
		"@prisma/client": "^4.8.0",
		"@sveltejs/adapter-node": "1.0.0",
		"@sveltejs/kit": "^1.0.1",
		"@tailwindcss/forms": "^0.5.3",
		"@trivago/prettier-plugin-sort-imports": "^4.0.0",
		"@trpc/client": "^10.7.0",
		"@trpc/server": "^10.7.0",
		"@types/autosize": "^4.0.1",
		"@typescript-eslint/eslint-plugin": "^5.47.0",
		"@typescript-eslint/parser": "^5.47.0",
		"autoprefixer": "^10.4.13",
		"autosize": "^5.0.2",
		"dayjs": "^1.11.7",
		"eslint": "^8.30.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"lucide-svelte": "^0.104.0",
		"nanoid": "^4.0.0",
		"postcss": "^8.4.20",
		"prettier": "^2.8.1",
		"prettier-plugin-svelte": "^2.9.0",
		"prettier-plugin-tailwindcss": "^0.2.1",
		"prisma": "^4.8.0",
		"superjson": "^1.12.1",
		"svelte": "^3.55.0",
		"svelte-check": "^3.0.1",
		"svelte-popperjs": "^1.3.2",
		"svelte-portal": "^2.2.0",
		"svelte-preprocess": "^5.0.0",
		"svelte-time": "^0.7.1",
		"tailwindcss": "3.2.4",
		"trpc-sveltekit": "^3.2.2",
		"ts-node": "^10.9.1",
		"tslib": "^2.4.1",
		"typescript": "^4.9.4",
		"vite": "^4.0.3",
		"zod": "^3.20.2"
	},
	"type": "module",
	"dependencies": {
		"socket.io": "^4.5.4",
		"socket.io-client": "^4.5.4"
	}
}